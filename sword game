local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()




local Window = Library.CreateLib("Sword game by NETFLIX", "DarkTheme")


local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")

Section:NewToggle("auto collect raid ticket", "", function (state)
    getgenv().SecondEgg = state
    if state then
        task.spawn(function ()
            while getgenv().SecondEgg and wait () do            
                local args = {
                    [1] = "Free Ticket 1"
                }
                
                game:GetService("ReplicatedStorage").Packages.Knit.Services.ChestService.RF.ClaimChest:InvokeServer(unpack(args))

                local args = {
                    [1] = "Free Ticket 2"
                }
                
                game:GetService("ReplicatedStorage").Packages.Knit.Services.ChestService.RF.ClaimChest:InvokeServer(unpack(args))

                local args = {
                    [1] = "Free Ticket 3"
                }
                
                game:GetService("ReplicatedStorage").Packages.Knit.Services.ChestService.RF.ClaimChest:InvokeServer(unpack(args))
                wait(60)
            end
            
        end)
    end
    
end)

local Tab = Window:NewTab("Settings")
local Section = Tab:NewSection("Settings")


Section:NewKeybind("Toggle the GUI", "Press P to toggle the GUI", Enum.KeyCode.P, function()
	Library:ToggleUI()
end)
